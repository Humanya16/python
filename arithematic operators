a=int(input())
b=int(input())
operator = input("Enter the operator (+, -, *, /, //, %): ")
if operator=='+':
    print(a+b)
elif operator=='-':
   print(a-b)
elif operator=='*':
    print(a*b)
elif operator=='/':
    print(a/b)
elif operator=='//':
    print(a//b)
elif operator=='%':
    print(a%b)
